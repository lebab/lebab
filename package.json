{
  "name": "lebab",
  "version": "2.7.3",
  "description": "Turn your ES5 code into readable ES6/ES7",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/ test/ system-test/ bin/ *.js",
    "prepare": "rm -rf lib/ && babel src/ --out-dir lib/",
    "postinstall": "npm run prepare && npm run build",
    "build": "./node_modules/.bin/gulp",
    "system-test": "mocha --compilers js:babel-register \"system-test/**/*Test.js\"",
    "//": "Unit tests: a) for single run, b) in watch-mode, c) with coverage.",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "mocha --compilers js:babel-register \"test/dev/**/*Test.js\"",
    "test:prod": "mocha \"test/prod/**/*Test.js\"",
    "watch": "mocha --watch --compilers js:babel-register \"test/dev/**/*Test.js\"",
    "coverage": "nyc npm test",
    "///": "These are used by Travis to create coverage report. Run 'coverage' script first.",
    "ensure-coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80",
    "upload-coverage": "nyc report --reporter=lcov && cat coverage/*/lcov.info | codecov"    
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mohebifar/lebab"
  },
  "bin": {
    "lebab": "./bin/index.js"
  },
  "keywords": [
    "es5",
    "es6",
    "es2015",
    "es7",
    "es2016",
    "transpiler",
    "transpile"
  ],
  "author": "Mohamad Mohebifar <mohamad@mohebifar.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohebifar/lebab/issues"
  },
  "homepage": "https://github.com/mohebifar/lebab",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "commander": "^2.9.0",
    "escope": "^3.6.0",
    "espree": "^3.4.2",
    "estraverse": "^4.1.1",
    "glob": "^7.0.5",
    "lodash": "^4.17.4",
    "recast": "^0.12.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "browserify": "^14.4.0",
    "chai": "^3.4.1",
    "codecov": "^1.0.1",
    "eslint": "^3.3.0",
    "eslint-plugin-no-null": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "uglify-es": "^3.0.26",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
